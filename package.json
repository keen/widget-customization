    "prettier": "^2.0.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.7.3",
    "react-test-renderer": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "standard-version": "^7.0.1",
    "styled-components": "^5",
    "terser-webpack-plugin": "^3.0.6",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.3.1"
